{{> navbar}}
<div class="jumbotron text-center">
    <h3>Mongo Scraper</h3>
</div>
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title"><strong>
                        Top Articles
                    </strong></h3>
                </div>
                <div class="panel-body" id="well-section">
                    {{#each articles}}
                    <article class="well" id="article-body">
                        <h3 class="articleTitle">{{this.title}}</h3>
                        <h5 class="articleLink">{{this.link}}</h5>
                        <h5>{{this.id}}</h5>
                    </article>
                    <br>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>