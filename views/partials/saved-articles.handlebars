{{#each articles}}{{#if saved}}
<div class="card">
    <div class="card-header bg-primary">
        <div class="header-left">
            <h3 class="articleTitle">{{title}}</h3>
        </div>
        <div class="header-right">
            <button type="button" class="btn notes-btn btn-success" data-id="{{_id}}">Notes</button>
            <button type="button" class="btn return-btn btn-danger" data-id="{{_id}}">Unsave</button>
        </div>
    </div>
    <div class="card-body">
        <a href="{{link}}" target="_blank">
            <h5 class="articleLink">{{link}}</h5>
        </a>
        <h5>{{_id}}</h5>
    </div>
</div>
<br>
{{/if}}{{/each}}